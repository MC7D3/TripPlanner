<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<VBox styleClass="form-container" stylesheets="@login_styles.css"
	xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tpgroup.view.javafx.JoinRoomGUIController">
	<children>
		<TextField fx:id="roomCodeTxt" promptText="Room's code" styleClass="input-field"
			onKeyPressed="#onFocus" />
		<Button fx:id="joinRoomTxt" styleClass="button-primary" text="Join Room"
			onAction="#joinRoom" />
		<Text fx:id="outLogTxt" strokeType="OUTSIDE" strokeWidth="0.0" />
	</children>
</VBox>
