<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<BorderPane style="-fx-min-width: 700;" styleClass="loginBackPane" stylesheets="@login_styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" styleClass="loginBackPane">
         <children>
            <VBox styleClass="form-pad">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-max-height: 30; -fx-alignment: center_left;" styleClass="form-container">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-size: 15; -fx-font-weight: normal;" styleClass="heading" text="Name" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-size: 15; -fx-font-weight: normal;" styleClass="heading" text="Location" />
                        <Region HBox.hgrow="ALWAYS" />
                        <Button mnemonicParsing="false" style="-fx-max-width: 110;" styleClass="button-primary" text="accept" />
                        <Button mnemonicParsing="false" style="-fx-min-width: 110;" styleClass="button-cancel" text="remove" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </StackPane>
   </center>
   <top>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="heading" text="Proposals" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-alignment: center_left;" styleClass="form-pad" BorderPane.alignment="CENTER">
         <children>
            <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" styleClass="button-primary" text="New " />
         </children>
      </HBox>
   </bottom>
</BorderPane>
