<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" styleClass="loginBackPane" stylesheets="@login_styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <HBox style="-fx-alignment: center_left;" styleClass="form-container" HBox.hgrow="ALWAYS">
               <children>
                  <Text fx:id="outLogTxt" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-size: 15; -fx-font-weight: normal;" styleClass="heading" text="room's code" />
                <Button fx:id="showProposalsBtn" mnemonicParsing="false" styleClass="button-primary" text="show proposals" />
                  <Button fx:id="goBackBtn" mnemonicParsing="false" styleClass="button-logout" text="go back" />
               </children>
            </HBox>
         </children>
      </HBox>
   </bottom>
   <center>
      <StackPane fx:id="graphPane" prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
